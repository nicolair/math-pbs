<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#associativite strophoide
restart;
with(geometry):with(plots):
_EnvHorizontalName := x: _EnvVerticalName := y:

#eq strophoide
a:=1;

etoile:= proc(A,B)
  local x1,y1,x2,y2,t,M,eq,eqd12,r;
  x1:=A[1];y1:=A[2];x2:=B[1];y2:=B[2];
  eq:=(a-x)*y^2-(a+x)*x^2;
  M:=[x1+t*(x2-x1),y1+t*(y2-y1)]:
  eqd12:= subs({x=x1+t*(x2-x1),y=y1+t*(y2-y1)},eq):
  eqd12:= sort(expand(eqd12),[t]):
  eqd12:=quo(eqd12,t,t):
  eqd12:=quo(eqd12,t-1,t):
  r:=solve({eqd12=0},{t}):
  subs(r,M);
end proc:

line(Ox,y=0):
line(Oy,x=0):
Itheta:= [-a*cos(2*theta)*cos(theta)/cos(theta), -a*cos(2*theta)*sin(theta)/cos(theta)]:

thetamax := Pi/4+0.4;
theta1 := -0.5;
theta2 := -0.1;
theta3 := 0.7;
lA1:=subs({theta=theta1},Itheta):
lA2:=subs({theta=theta2},Itheta):
lA12 := etoile(lA1,lA2):
lA3:=subs({theta=theta3},Itheta):
lA23 := etoile(lA2,lA3):
lP:=etoile(lA12,lA3);
lPP:=etoile(lA1,lA23);

point(A1,lA1):
point(A2,lA2):
point(A3,lA3):
point(A12,lA12):
point(A23,lA23):
line(d12,[A1,A2]):
line(d23,[A2,A3]):

para:=view=[-1.5..1,-2..2],scaling=constrained;
dess1:=plot([-a*cos(2*theta)*cos(theta)/cos(theta), -a*cos(2*theta)*sin(theta)/cos(theta),theta=-thetamax..thetamax],para):
dess2:=draw([A1,A2,A3,d12,d23,A12,A23],para):

display([dess1,dess2]);

</Text-field>

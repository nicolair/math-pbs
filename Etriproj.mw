<Text-field prompt="&gt; " style="Maple Input" layout="_pstyle1">restart;
with(LinearAlgebra):
with(geom3d):with(plots):
# attention par rapport a l'enonce les roles de a,b,c et u,v,w sont inverses
<Font encoding="UTF-8">   #donn\303\251es du triangle</Font>
point(O,0,0,0):
r0:=3:
point(II,r0,0,0):point(J,0,r0,0):point(K,0,0,r0):
M:=[r*sin(phi)*cos(theta),r*sin(phi)*sin(theta),r*cos(phi)]:
r1:=2.5:tt:=-0.2:
cA:=subs({r=r1,theta=1.1+tt,phi=0.5},M):
cB:=subs({r=r1,theta=2.5+tt,phi=1.05},M):
cC:=subs({r=r1,theta=-1+tt,phi=0.5},M):
point(A,op(cA)):point(B,op(cB)):point(C,op(cC)):
vA:=&lt;op(cA)&gt;:vB:=&lt;op(cB)&gt;:vC:=&lt;op(cC)&gt;:
segment(OA,O,A):segment(OB,O,B):segment(OC,O,C):
segment(OI,O,II):segment(OJ,O,J):segment(OK,O,K):
triangle(OAB,[O,A,B]):triangle(OBC,[O,B,C]):triangle(OCA,[O,C,A]):

#plan hauteur issu de A
point(W,CrossProduct(vB,vC)[1],CrossProduct(vB,vC)[3],CrossProduct(vB,vC)[3]):
plane(planorth,[O,A,W]):
dessin1:=
  draw([OA(color=red),OB(color=red),OC(color=red),
      OI(color=black),OJ(color=black),OK(color=black),
      OCA,OAB,OBC,
      planorth(color=yellow, style=patchnogrid)
     ],
      orientation=[45,45],axes=none):<Font style="_pstyle1">
display(dessin1);
</Font>
#plan bissecteur issu de A
v1:=CrossProduct(vA,vB):
v1:=VectorScalarMultiply(v1, 1 / VectorNorm(v1,Euclidean)):
v2:=CrossProduct(vA,vC):  #attention a l'interversion
v2:=VectorScalarMultiply(v2, 1 / VectorNorm(v2,Euclidean)):
vq:=&lt;x,y,z&gt;:
Eq:=DotProduct(Add(v1,v2),vq):
plane(planbiss,Eq,[x,y,z]):
dessin2:=
  draw([OA(color=red),OB(color=red),OC(color=red),
      OI(color=black),OJ(color=black),OK(color=black),
      OCA,OAB,OBC,
      planbiss(color=yellow, style=patchnogrid)
     ],
      orientation=[45,45],axes=none):
display(dessin2);

#plan mediateur de  OA, OB
v1:=VectorScalarMultiply(vB, 1 / VectorNorm(vB,Euclidean)):
v2:=VectorScalarMultiply(vC, -1 / VectorNorm(vC,Euclidean)): #attention au signe
vq:=&lt;x,y,z&gt;:
Eq:=DotProduct(Add(v1,v2),vq):
plane(planmed,Eq,[x,y,z]):
dessin3:=
  draw([OA(color=red),OB(color=red),OC(color=red),
      OI(color=black),OJ(color=black),OK(color=black),
      OCA,OAB,OBC,
      planmed(color=yellow, style=patchnogrid)
     ],
      orientation=[45,45],axes=none):
display(dessin3);


<Group labelreference="L3">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart;
a:=1;
with(geometry):with(plots):
A:=(3*tau^2-1)*(1-t^2)+(tau^3-3*tau)*2*t+(1+t^2)^2;
expand(subs(t=T+tau,A));

eqt:=(t^3-3*t)*y+(3*t^2-1)*x+2*a:
N:=solve({subs({t=t1},eqt),subs({t=t2},eqt)},{x,y}):
N:=simplify(N,{t1*t2=3,t1+t2=-2*tau});
eliminate(N,{tau});

a:=1;
N:=subs(N,[x,y]);
M:=[2*(1-t^2)/(1+t^2)^2,4*t/(1+t^2)^2];

optionsd:=axes=none,scaling=constrained:
hyperbole:=plot([op(N),tau=-3.1..3.1],color=blue):
cardioide:=plot([op(M),t=-10..10]):

V:=diff(M,t):
M1:=subs([solve(t^2+2*tau*t+3,{t})][1],M):
M2:=subs([solve(t^2+2*tau*t+3,{t})][2],M):

Mtau:=subs(t=tau,M):
Ntau:=subs(t=tau,N):

tau:=2.3;
A:=Mtau+(-1.6)*(M1-M2);
B:=Mtau+(1.6)*(M1-M2);
tangente:=plot([A,B],color=turquoise):
NtauM1:=plot([Ntau,M1],color=yellow):
NtauM2:=plot([Ntau,M2],color=yellow):

tau:=-2.9;
A:=Mtau+(-1.6)*(M1-M2);
B:=Mtau+(1.6)*(M1-M2);
tangente_2:=plot([A,B],color=turquoise):
NtauM1_2:=plot([Ntau,M1],color=yellow):
NtauM2_2:=plot([Ntau,M2],color=yellow):

display([cardioide,hyperbole,tangente,NtauM1,NtauM2,tangente_2,NtauM1_2,NtauM2_2]);
